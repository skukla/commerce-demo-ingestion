# Data Repository Path
# Path to the data repository containing definitions/ and generated/ datapacks
# Project configuration is loaded from {DATA_REPO_PATH}/definitions/project.json
DATA_REPO_PATH=../buildright-data

# Adobe Commerce Configuration
COMMERCE_BASE_URL=https://your-commerce-instance.com
COMMERCE_ADMIN_USERNAME=admin
COMMERCE_ADMIN_PASSWORD=your-password

# Import Options
BATCH_SIZE=50
CONCURRENCY=5

# Adobe Commerce Optimizer (ACO) Configuration
ACO_API_URL=https://your-aco-instance.com
ACO_API_KEY=your-api-key
ACO_TENANT_ID=your-tenant-id
ACO_ENVIRONMENT_ID=your-environment-id

